// Ejercicio 1: Comprobacion de edad

let int age = readInt("¿Cuantos años tienes?: ");

if age > 0 and age < 120
{
    if age >= 18
    {
        writeline("Eres un adulto");
    }
    else
    {
        writeline("Eres un menor de edad");
    }
}
else
{
    writeline("Tu edad no puede ser negativa o mayor a 120");
}