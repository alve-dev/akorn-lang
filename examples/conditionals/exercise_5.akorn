// Conditionals, exercise 5: Comprobacion tributaria

let int age = readInt("¿Cual es tu edad?: ");
let float monthly_salary = readFloat("Cuanto son tus ingresos mensuales?: ");

if age > 0 and age < 100
{
    if age > 16 and monthly_salary >= 1000
    {
        writeline("\n¿Tienes que tributar?: SI");
    }
    else
    {
        writeline("\n¿Tienes que tributar?: NO");
    };
}
else
{
    writeline("Tu edad no puede ser negativa, mayor de 100 o 0");
}